# EditorConfig para consistency de archivos
# https://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{js,ts,tsx,jsx,json,css}]
indent_size = 2

[*.md]
trim_trailing_whitespace = false
